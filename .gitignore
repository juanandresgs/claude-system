projects/
todos/
shell-snapshots/
backups/
history/

# Runtime and metrics
reports/
metrics/
statsig/

# Local configuration
settings.local.json

# Temporary files
*.tmp
*.log
*.bak
.DS_Store

# IDE and editor files
.vscode/
.idea/
*.swp
*.swo
*~

